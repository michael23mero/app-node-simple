<title><%= title%></title>
<%- include('layout/header')%>
<div class="container mt-5 py-5">
    <div class="row mt-4">
        <div class="col-md-5 mx-auto">
            <div class="card p-4 shadow text-center">
                <form action="/login" method="POST">
                    <h5>Iniciar sesion</h5><hr>
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Ingrese su username" name="username">
                    </div>
                    <div class="form-group">
                        <input type="password" class="form-control" placeholder="Ingrese su password" name="password">
                    </div>
                    <button class="btn btn-dark btn-block">Iniciar Sesion</button><hr>
                    <a href="/login2" class="btn">- Doble autenticacion -</a>
                </form>
            </div>
        </div>
    </div>
</div>
<%- include('layout/footer')%>
