<title><%= title%></title>
<%- include('layout/header')%>
<body>
    <div class="container mt-5 py-5">
        <div class="row mt-4">
            <div class="col-md-5 mx-auto">
                <div class="card p-4 shadow">
                    <form action="/login" method="POST">
                        <h5 class="text-center">Iniciar sesion</h5><hr>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Ingrese su username" name="username">
                        </div>
                        <div class="form-group">
                            <input type="password" class="form-control" placeholder="Ingrese su password" name="password">
                        </div>
                        <button class="btn btn-dark btn-block">Iniciar Sesion</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
<%- include('layout/footer')%>
</body>
